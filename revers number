#include<stdio.h>
int main(){
	int n,sum, revers;
	printf("Enter a number n: ");
	scanf("%d", &n);
	while(n!=0) {
	revers =+ revers*10 +(n%10);
	n =n/10;	
	} 
	printf("%d", revers);                                                                                                                                                                                        

	return 0;
}
